{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["DOM", "ES5", "ES6"],
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "removeComments": true,
    "downlevelIteration": true,
    "sourceMap": false,
    "typeRoots": ["node_modules/@types", "./typings/*"]
  },
  "include": ["src", "test"]
}
